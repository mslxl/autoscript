S      ::= expr
expr   ::= equality

equality ::= relational "==" relational | relational "!=" relational | relational
relational ::= add "<" add | add "<=" add | add ">" add | add ">=" add | add

add    ::= add '+' mul | add '-' mul | mul
mul    ::= mul '*' unary | mul '/' unary | unary

unary  ::= '-' num | '+' num | num
num    ::= integer | (add)
